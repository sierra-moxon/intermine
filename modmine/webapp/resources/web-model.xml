<!-- this comment is here to prevent ant from setting model.web to null -->

<servlet>
  <servlet-name>modmine-metadata-cache-query</servlet-name>
  <servlet-class>org.modmine.webservice.MetadataCacheQueryServlet</servlet-class>
  <init-param>
     <param-name>debug</param-name>
     <param-value>true</param-value>
  </init-param>
</servlet>

<servlet-mapping>
  <servlet-name>modmine-metadata-cache-query</servlet-name>
  <url-pattern>/service/query/metadatacache/*</url-pattern>
</servlet-mapping>
