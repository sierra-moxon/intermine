<?xml version="1.0"?>
<classes>

  <class name="org.flymine.model.genomic.Provider" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="surnamePI" type="java.lang.String"/>
    <attribute name="namePI" type="java.lang.String"/>
    <attribute name="studyTitle" type="java.lang.String"/>
    <reference name="experimentSubmission" referenced-type="org.flymine.model.genomic.ExperimentSubmission" reverse-reference="provider"/>
  </class>

  <class name="org.flymine.model.genomic.ExperimentSubmission" extends="org.flymine.model.genomic.DataSet" is-interface="true">
    <attribute name="title" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
    <attribute name="design" type="java.lang.String"/>
    <attribute name="factorType" type="java.lang.String"/>
    <attribute name="qualityControl" type="java.lang.String"/>
    <attribute name="replicate" type="java.lang.String"/>
    <attribute name="experimentDate" type="java.lang.String"/>
    <attribute name="publicationDate" type="java.lang.String"/>
    <publication name="publication" referenced-type="org.flymine.model.genomic.Publication"/>
    <reference name="organism" referenced-type="org.flymine.model.genomic.Organism"/>
<!-- 
    <attribute name="fileFormat" type="java.lang.String"/>
-->
    <reference name="provider" referenced-type="org.flymine.model.genomic.Provider" reverse-reference="experimentSubmission"/>
    <collection name="submissionData" referenced-type="org.flymine.model.genomic.SubmissionData" reverse-reference="experimentSubmission"/>
    <collection name="protocol" referenced-type="org.flymine.model.genomic.Protocol" reverse-reference="experimentSubmission"/>
  </class>


  <class name="org.flymine.model.genomic.Protocol" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
    <attribute name="type" type="java.lang.String"/>

<!-- 
    <attribute name="references" type="java.lang.String"/>
-->
    <collection name="experimentSubmission" referenced-type="org.flymine.model.genomic.ExperimentSubmission" reverse-reference="protocol"/>
  </class>


  <class name="org.flymine.model.genomic.SubmissionData" is-interface="true">

    <attribute name="type" type="java.lang.String"/>

    <attribute name="transcriptId" type="java.lang.String"/>
    <attribute name="innerPrimer" type="java.lang.String"/>
    <attribute name="traceArchiveId" type="java.lang.String"/>
    <attribute name="geneBankId" type="java.lang.String"/>
    <attribute name="label" type="java.lang.String"/>

    <attribute name="arrayDesignRef" type="java.lang.String"/>
    <attribute name="hybridization" type="java.lang.String"/>
    <attribute name="source" type="java.lang.String"/>
    <attribute name="sample" type="java.lang.String"/>
    <attribute name="extract" type="java.lang.String"/>
    <attribute name="labelExtract" type="java.lang.String"/>
    <attribute name="arrayDataFile" type="java.lang.String"/>
    <attribute name="ArrayMatrixDataFile" type="java.lang.String"/>

<!-- 
    <attribute name="name" type="java.lang.String"/>
    <attribute name="heading" type="java.lang.String"/>
    <attribute name="value" type="java.lang.String"/>
    -->
    
    <reference name="experimentSubmission" referenced-type="org.flymine.model.genomic.ExperimentSubmission" reverse-reference="submissionData"/>
  </class>

</classes>
