# svn directory
svndirectory = ~/svn/model_update

# Mine name
mine = flymine

# files to update
inputfile = $svndirectory/$mine/dbmodel/resources/genomic_keyDefs.properties
inputfile = $svndirectory/$mine/dbmodel/resources/objectstoresummary.config.properties
inputfile = $svndirectory/$mine/dbmodel/resources/class_keys.properties
inputfile = $svndirectory/$mine/dbmodel/resources/genomic_precompute.properties
