<model name="logmodel">

    <class name="org.intermine.model.logmodel.InterMineLoggable" is-interface="false">
        <attribute name="caller" type="java.lang.String"/>
        <attribute name="message" type="java.lang.String"/>
        <attribute name="timestamp" type="java.lang.Long"/>
    </class>

    <class name="org.intermine.model.logmodel.LoggableQuery" extends="org.intermine.model.logmodel.InterMineLoggable" is-interface="false">
        <attribute name="initiator" type="java.lang.String"/>
        <attribute name="queryOQL" type="java.lang.String"/>
        <attribute name="querySQL" type="java.lang.String"/>
        <attribute name="optimise" type="java.lang.Long"/>
        <attribute name="estimated" type="java.lang.Long"/>
        <attribute name="execute" type="java.lang.Long"/>
        <attribute name="acceptable" type="java.lang.Long"/>
        <attribute name="conversion" type="java.lang.Long"/>
    </class>

</model>
