
<project name="flymine-dbmodel" default="default" basedir=".">
  
  <description>represents the production objectstore and the model therein</description>
  <import file="../../imbuild/objectstore.xml"/>

  <target name="-merge-models" unless="merge.not.required">
    <merge-additions source="bio/sources/so/so_additions.xml"/>
    <merge-additions source="bio/sources/psi/psi_additions.xml"/>
    <merge-additions source="bio/sources/rnai/rnai_additions.xml"/>
    <merge-additions source="bio/sources/ensembl/ensembl_additions.xml"/>
    <merge-additions source="bio/sources/phenotype/phenotype_additions.xml"/>
    <merge-additions source="bio/sources/go/go_additions.xml"/>
    <merge-additions source="bio/sources/go-annotation/go-annotation_additions.xml"/>
    <merge-additions source="bio/sources/uniprot/uniprot_additions.xml"/>
    <merge-additions source="bio/sources/protein_structure/protein_structure_additions.xml"/>
    <merge-additions source="bio/sources/orthologue/orthologue_additions.xml"/>
    <merge-additions source="bio/sources/flyreg/flyreg_additions.xml"/>
    <merge-additions source="bio/sources/tiling_path/tiling_path_additions.xml"/>
    <merge-additions source="bio/sources/long_oligo/long_oligo_additions.xml"/>
    <merge-additions source="bio/sources/flybase-gff/flybase-gff_additions.xml"/>
    <merge-additions source="bio/sources/homophila/homophila_additions.xml"/>
    <merge-additions source="bio/sources/interpro/interpro_additions.xml"/>
    <merge-additions source="bio/sources/cdnaclone/cdnaclone_additions.xml"/>
    <merge-additions source="bio/sources/drosdel/drosdel_additions.xml"/>
    <merge-additions source="bio/sources/mage/mage_additions.xml"/>
  </target>

</project>

