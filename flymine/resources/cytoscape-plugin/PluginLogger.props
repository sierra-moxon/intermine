# Set root logger level to DEBUG and its only appender to A1.
log4j.rootLogger=WARN, A1
log4j.logger.org.flymine.plugin=WARN
log4j.logger.org.flymine=WARN
log4j.logger.org.flymine.networkview.FlyNetworkIntegrator=INFO
log4j.logger.org.intermine.objectstore.webservice=WARN

# A1 is set to be a ConsoleAppender.
log4j.appender.A1=org.apache.log4j.ConsoleAppender
log4j.appender.A1.layout=org.apache.log4j.PatternLayout
log4j.appender.A1.layout.ConversionPattern=%d{dd/MM/yy,HH:mm:ss} %-5p %37c %3x - %m%n


