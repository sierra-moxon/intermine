<!-- this comment is here to prevent ant from setting model.struts.config to null -->
  	
               
    <action path="/sequenceExporter"
            type="org.flymine.web.SequenceExporter"/>
    
    <action path="/chartRenderer"
            type="org.flymine.web.ChartRenderer"
            parameter="method"/>
    
    <action path="/quickStart" forward="flymineQuickStart.page"/>
    
    <action path="/initGeneMicroArrayDisplayer"
            type="org.flymine.web.GeneMicroArrayDisplayerController"/>

    <action path="/proteinInteractionRenderer"
            type="org.flymine.web.ProteinInteractionImgRenderer"/>
    
	<action path="/initOrthologueDisplayer" 
		type="org.flymine.web.OrthologueDisplayerController"/>
		
    <action path="/orthologueAction"
            type="org.flymine.web.OrthologueAction">
      <forward name="results" path="/results.do" redirect="true"/>
      <forward name="details" path="/objectDetails.do" redirect="true"/>
    </action>
    
	<action path="/initGoStatDisplayer" 
		type="org.flymine.web.GoStatDisplayerController" />
    
    <action path="/goStatDisplayer" 
		forward="goStatDisplayer.tile" />
    
    <action path="/goStatAction"
            type="org.flymine.web.GoStatAction">
      <forward name="results" path="/results.do" redirect="true"/>    
    </action>
    
    <action path="/initProteinDomainDisplayer" 
		type="org.flymine.web.ProteinDomainDisplayerController" />
    
    <action path="/proteinDomainDisplayer" 
		forward="proteinDomainDisplayer.tile" />
    
    <action path="/proteinDomainAction"
            type="org.flymine.web.ProteinDomainAction">
      <forward name="results" path="/results.do" redirect="true"/>    
    </action>
    
    <action path="/initPublicationDisplayer" 
		type="org.flymine.web.PublicationDisplayerController" />
    
    <action path="/publicationDisplayer" 
		forward="publicationDisplayer.tile" />
    
    <action path="/publicationAction"
            type="org.flymine.web.PublicationAction">
      <forward name="results" path="/results.do" redirect="true"/>    
    </action>
    
    