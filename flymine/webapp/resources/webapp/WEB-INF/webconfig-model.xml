                                 filters=""
                                 filterLabel=""
                                 max="0.10"
                                 typeClass="org.flymine.model.genomic.Gene"
                                 externalLink="" />

      <bagtabledisplayer id="kegg" title="Pathway Information (KEGG)" pathStrings="Gene.pathways"
