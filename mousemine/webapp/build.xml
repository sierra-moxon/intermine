<project name="mousemine-webapp" default="default" basedir=".">

  <description>build, test, package mousemine-webapp</description>
  <import file="../../imbuild/application.xml"/>
  <target name="-pre-jar" 
          depends="application.-pre-jar">
    <ant antfile="build.xml" dir="../dbmodel" target="summarise-objectstore"/>
  </target>

  <!-- it's hacky to override do-generate, but it gets run at the right time -->
  <target name="do-generate">
    <ant antfile="build.xml" dir="../dbmodel" target="summarise-objectstore" inheritAll="false"/>
  </target>

</project>
