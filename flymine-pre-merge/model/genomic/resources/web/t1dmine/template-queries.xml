<template-list>
  <template name="AAA_allHumanGenes" description="Show all the Human genes." important="false" keywords="Genomics">
    <query name="AAA_allHumanGenes" model="genomic" view="Gene">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.organism" type="Organism">
      </node>
      <node path="Gene.organism.name" type="String">
        <constraint op="=" value="Homo sapiens" description="Show all the genes from:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAB_Gene2Synonym1" description="Show the synonyms and synonym source for a particular gene." important="false" keywords="Genomics">
    <query name="AAB_Gene2Synonym1" model="genomic" view="Synonym.subject Synonym.subject.synonyms.value Synonym.subject.synonyms.type Synonym.subject.synonyms.source.title">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="ENSG00000182652" description="Show the synonyms for gene:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
    </query>
  </template>
  <template name="AAC_diseaseRegion2Genes" description="Search for genes within a particular disease region and show their GO terms." important="false" keywords="Genomics">
    <query name="AAC_diseaseRegion2Genes" model="genomic" view="Gene Gene.objects.start Gene.objects.end Gene.GOTerms.identifier">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.objects" type="Location">
      </node>
      <node path="Gene.objects.object" type="Chromosome">
      </node>
      <node path="Gene.objects.object.identifier" type="String">
        <constraint op="=" value="1" description="Show genes on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Gene.objects.start" type="Integer">
        <constraint op="&gt;" value="107387119" description="between:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Gene.objects.end" type="Integer">
        <constraint op="&lt;" value="110100100" description="and:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAD_Genes2chromosome1" description="Show the genes located on a particular chromsome." important="false" keywords="Genomics">
    <query name="AAD_Genes2chromosome1" model="genomic" view="Gene.name Gene.organismDbId Gene.chromosome.identifier">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.chromosome" type="Chromosome">
      </node>
      <node path="Gene.chromosome.identifier" type="String">
        <constraint op="=" value="2L" description="Show genes located on chromosome" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Gene.organism" type="Organism">
      </node>
      <node path="Gene.organism.name" type="String">
        <constraint op="=" value="Homo Sapiens" description="from" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAD_chromosomeLocation2genes3" description="Give the location of genes on a particular chromsome." important="false" keywords="Genomics">
    <query name="AAD_chromosomeLocation2genes3" model="genomic" view="Chromosome.identifier Chromosome.genes.objects.strand Chromosome.genes.objects.start Chromosome.genes.objects.end Chromosome.genes.organismDbId">
      <node path="Chromosome" type="Chromosome">
      </node>
      <node path="Chromosome.identifier" type="String">
        <constraint op="=" value="1" description="Give the location of genes on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Chromosome.genes" type="Gene">
      </node>
      <node path="Chromosome.genes.objects" type="Location">
      </node>
      <node path="Chromosome.genes.objects.object" type="Chromosome">
      </node>
    </query>
  </template>
  <template name="AAE_chromosomeLocation2genes2" description="Show the genes, transcripts and exons located between two points on a chromosome. Give the location of the exons." important="false" keywords="Genomics">
    <query name="AAE_chromosomeLocation2genes2" model="genomic" view="Chromosome.identifier Chromosome.exons.gene.name Chromosome.exons.gene.organismDbId Chromosome.exons.gene.identifier Chromosome.exons.transcripts.identifier Chromosome.exons.identifier Chromosome.exons.objects.start Chromosome.exons.objects.end Chromosome.exons.objects.strand">
      <node path="Chromosome" type="Chromosome">
      </node>
      <node path="Chromosome.identifier" type="String">
        <constraint op="=" value="1" description="Show the genes, transcripts and exons located on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Chromosome.exons" type="Exon">
      </node>
      <node path="Chromosome.exons.objects" type="Location">
      </node>
      <node path="Chromosome.exons.objects.object" type="Chromosome">
      </node>
      <node path="Chromosome.exons.objects.start" type="Integer">
        <constraint op="&gt;" value="1" description="between base:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Chromosome.exons.objects.end" type="Integer">
        <constraint op="&lt;" value="10000" description="and:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAF_gene2Transcripts2" description="Show the predicted transcripts and their chromosomal location for a particular gene." important="false" keywords="Genomics">
    <query name="AAF_gene2Transcripts2" model="genomic" view="Gene Gene.organismDbId Gene.identifier Gene.transcripts.identifier Gene.transcripts.objects.object.identifier Gene.transcripts.objects.start Gene.transcripts.objects.end Gene.transcripts.objects.strand">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.synonyms" type="Synonym">
      </node>
      <node path="Gene.synonyms.value" type="String">
        <constraint op="=" value="ENSG00000182732" description="Show the predicted transcripts and chromosomal location for gene:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="Gene.transcripts" type="Transcript">
      </node>
      <node path="Gene.transcripts.objects" type="Location">
      </node>
      <node path="Gene.transcripts.objects.object" type="Chromosome">
      </node>
    </query>
  </template>
  <template name="AAG_synonymGene2exonChromosomeLocations2" description="Show the chromosome location of the exons for a particular gene" important="false" keywords="Genomics">
    <query name="AAG_synonymGene2exonChromosomeLocations2" model="genomic" view="Synonym.subject.name Synonym.subject.organismDbId Synonym.subject.chromosome.identifier Synonym.subject.exons.identifier Synonym.subject.exons.objects.start Synonym.subject.exons.objects.end">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="zen" description="Show the chromosome location of the exons of gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.exons" type="Exon">
      </node>
      <node path="Synonym.subject.exons.objects" type="Location">
      </node>
      <node path="Synonym.subject.exons.objects.object" type="Chromosome">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Homo Sapiens" description="from organism:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAH_GOterm2Proteins" description="Show the proteins annotated with a particular GO term." important="false" keywords="Genomics">
    <query name="AAH_GOterm2Proteins" model="genomic" view="Protein Protein.annotations.property Protein.primaryAccession Protein.annotations.property.identifier">
      <node path="Protein" type="Protein">
      </node>
      <node path="Protein.annotations" type="Annotation">
      </node>
      <node path="Protein.annotations.property" type="GOTerm">
      </node>
      <node path="Protein.annotations.property.identifier" type="String">
        <constraint op="=" value="GO:0008270" description="Show the proteins annotated with GO term:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAI_Gene2GO2" description="Show the GO annotations for a particular protein (table result)." important="false" keywords="Genomics">
    <query name="AAI_Gene2GO2" model="genomic" view="Protein.primaryAccession Protein.annotations.property.identifier">
      <node path="Protein" type="Protein">
      </node>
      <node path="Protein.annotations" type="Annotation">
      </node>
      <node path="Protein.annotations.property" type="GOTerm">
      </node>
      <node path="Protein.primaryAccession" type="String">
        <constraint op="=" value="O00455" description="Show the GO annotation for protein (uniprot id):" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="AAJ_Protein2GOterms1" description="Show the GO annotations for a particular protein (object result)." important="false" keywords="Genomics">
    <query name="AAJ_Protein2GOterms1" model="genomic" view="Protein Protein.annotations.property">
      <node path="Protein" type="Protein">
      </node>
      <node path="Protein.annotations" type="Annotation">
      </node>
      <node path="Protein.annotations.property" type="GOTerm">
      </node>
      <node path="Protein.primaryAccession" type="String">
        <constraint op="=" value="O00110" description="Show GO terms annotated to protein:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="Object_Synonym" description="Find an entry in the database using any identifier or synonym." important="false" keywords="Genomics">
    <query name="Object_Synonym" model="genomic" view="Synonym.subject">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="timeout" description="" identifier="Synonym.value.1" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="TFBScluster2expression" description="Show genes within a certain distance of TFBS clusters which are up/down regulated in the 'Wagner' gene expression experiment." important="false" keywords="Genomics Gene Expression">
    <query name="TFBScluster2expression" model="genomic" view="TFBSCluster.geneDistances.subject TFBSCluster.geneDistances.subject.comment.text TFBSCluster.geneDistances.subject.microArrayResults.value TFBSCluster.geneDistances.subject.microArrayResults.assay.name TFBSCluster.geneDistances.subject.microArrayResults.assay.sample1 TFBSCluster.geneDistances.subject.microArrayResults.assay.sample2">
      <node path="TFBSCluster" type="TFBSCluster">
      </node>
      <node path="TFBSCluster.geneDistances" type="DistanceRelation">
      </node>
      <node path="TFBSCluster.geneDistances.distance" type="Integer">
        <constraint op="&lt;=" value="10000" description="Show genes within a distance of:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="TFBSCluster.geneDistances.subject" type="Gene">
      </node>
      <node path="TFBSCluster.geneDistances.subject.microArrayResults" type="MicroArrayResult">
      </node>
      <node path="TFBSCluster.geneDistances.subject.microArrayResults.value" type="Float">
        <constraint op="&gt;=" value="2.0" description="which have a normalised log ratio of:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="TFBSCluster.geneDistances.subject.microArrayResults.assay" type="MicroArrayAssay">
      </node>
      <node path="TFBSCluster.geneDistances.subject.microArrayResults.assay.sample1" type="String">
        <constraint op="LIKE" value="%(FDF)%" description="In assay:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="TFBSCluster.geneDistances.subject.symbol" type="String">
        <constraint op="IS NOT NULL" value="null" description="" identifier="">
        </constraint>
      </node>
    </query>
  </template>
  <template name="chrLocation2CpGislands" description="Show CpG islands located in a particular chromsome location." important="false" keywords="Genomics">
    <query name="chrLocation2CpGislands" model="genomic" view="CpGIsland">
      <node path="CpGIsland" type="CpGIsland">
      </node>
      <node path="CpGIsland.objects" type="Location">
      </node>
      <node path="CpGIsland.objects.object" type="Chromosome">
      </node>
      <node path="CpGIsland.objects.object.identifier" type="String">
        <constraint op="=" value="1" description="Show CpG islands location on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="CpGIsland.objects.start" type="Integer">
        <constraint op="&gt;" value="1" description="between:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="CpGIsland.objects.end" type="Integer">
        <constraint op="&lt;" value="10000" description="and:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="chrLocation2DiseaseRegion" description="Show any defined disease regions within a certain chromosome location." important="false" keywords="Genomics">
    <query name="chrLocation2DiseaseRegion" model="genomic" view="DefinedRegion">
      <node path="DefinedRegion" type="DefinedRegion">
      </node>
      <node path="DefinedRegion.type" type="String">
        <constraint op="=" value="diseaseRegion" description="Show regions of type:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="DefinedRegion.objects" type="Location">
      </node>
      <node path="DefinedRegion.objects.object" type="Chromosome">
      </node>
      <node path="DefinedRegion.objects.object.identifier" type="String">
        <constraint op="=" value="1" description="on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="DefinedRegion.objects.start" type="Integer">
        <constraint op="&gt;" value="100000000" description="between:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="DefinedRegion.objects.end" type="Integer">
        <constraint op="&lt;" value="140000000" description="and:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="chrLocation2TFBindingSites" description="Show transcription factor binding sites that lie within a particular chromsome location." important="false" keywords="Genomics">
    <query name="chrLocation2TFBindingSites" model="genomic" view="TFBindingSite">
      <node path="TFBindingSite" type="TFBindingSite">
      </node>
      <node path="TFBindingSite.objects" type="Location">
      </node>
      <node path="TFBindingSite.objects.object" type="Chromosome">
      </node>
      <node path="TFBindingSite.objects.object.identifier" type="String">
        <constraint op="=" value="1" description="Show transcription factor binding sites on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="TFBindingSite.objects.start" type="Integer">
        <constraint op="&gt;" value="1" description="where:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="TFBindingSite.objects.end" type="Integer">
        <constraint op="&lt;" value="15000" description="and" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="chrLocation2encodeRegion" description="Show any defined encode regions within a certain chromosome location." important="false" keywords="Genomics">
    <query name="chrLocation2encodeRegion" model="genomic" view="DefinedRegion">
      <node path="DefinedRegion" type="DefinedRegion">
      </node>
      <node path="DefinedRegion.type" type="String">
        <constraint op="=" value="encodeRegion" description="Show regions of type:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="DefinedRegion.objects" type="Location">
      </node>
      <node path="DefinedRegion.objects.object" type="Chromosome">
      </node>
      <node path="DefinedRegion.objects.object.identifier" type="String">
        <constraint op="=" value="1" description="on chromosome:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="DefinedRegion.objects.start" type="Integer">
        <constraint op="&gt;" value="120000000" description="from:" identifier="" editable="true">
        </constraint>
      </node>
      <node path="DefinedRegion.objects.end" type="Integer">
        <constraint op="&lt;" value="150000000" description="to:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="factor2TFBindingsite" description="Show transcription factor binding sites which bind a particular factor." important="false" keywords="Genomics">
    <query name="factor2TFBindingsite" model="genomic" view="TFBindingSite">
      <node path="TFBindingSite" type="TFBindingSite">
      </node>
      <node path="TFBindingSite.factors" type="Protein">
      </node>
      <node path="TFBindingSite.factors.primaryAccession" type="String">
        <constraint op="=" value="Q02078" description="Show transcription factor binding sites which bind factor:" identifier="" editable="true">
        </constraint>
      </node>
    </query>
  </template>
</template-list>