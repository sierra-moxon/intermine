<?xml version="1.0"?>
<classes>

    <class name="org.flymine.model.genomic.CDS" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"
                   reverse-reference="CDSs"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein"/>
    </class>

    <class name="org.flymine.model.genomic.Exon" is-interface="true">
        <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript"
                    reverse-reference="exons" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.Enhancer" is-interface="true">
    </class>

    <class name="org.flymine.model.genomic.FivePrimeUTR" extends="org.flymine.model.genomic.UTR" is-interface="true">
    </class>

    <class name="org.flymine.model.genomic.Gene" is-interface="true">
       <attribute name="ncbiGeneNumber" type="java.lang.String"/>
       <collection name="UTRs" referenced-type="org.flymine.model.genomic.UTR" reverse-reference="gene" ordered="true"/>
        <collection name="CDSs" referenced-type="org.flymine.model.genomic.CDS" reverse-reference="gene" ordered="true"/>
        <collection name="publications" referenced-type="org.flymine.model.genomic.Publication" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.Intron" is-interface="true">
        <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript" reverse-reference="introns" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.UTR" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="UTRs"/>
        <reference name="MRNA" referenced-type="org.flymine.model.genomic.MRNA" reverse-reference="UTRs"/>
    </class>

    <class name="org.flymine.model.genomic.Transcript" is-interface="true">
        <attribute name="exonCount" type="java.lang.Integer"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein" reverse-reference="transcripts"/>
        <collection name="introns" referenced-type="org.flymine.model.genomic.Intron" ordered="true" reverse-reference="transcripts"/>
        <collection name="exons" referenced-type="org.flymine.model.genomic.Exon" reverse-reference="transcripts" ordered="true"/>

    </class>

    <class name="org.flymine.model.genomic.NcRNA" is-interface="true"/>

    <class name="org.flymine.model.genomic.Protein" is-interface="true">
        <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript" reverse-reference="protein" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.ProteinBindingSite" is-interface="true"/>

    <class name="org.flymine.model.genomic.Pseudogene" is-interface="true"/>

    <class name="org.flymine.model.genomic.TransposableElementInsertionSite" is-interface="true"/>

    <class name="org.flymine.model.genomic.InsertionSite" is-interface="true">
        <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.MRNA" extends="org.flymine.model.genomic.Transcript"  is-interface="true">
        <reference name="fivePrimeUTR" referenced-type="org.flymine.model.genomic.FivePrimeUTR"/>
        <reference name="threePrimeUTR" referenced-type="org.flymine.model.genomic.ThreePrimeUTR"/>
        <collection name="UTRs" referenced-type="org.flymine.model.genomic.UTR" reverse-reference="MRNA" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.SequenceVariant" is-interface="true">
        <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.TransposableElement" is-interface="true">
    </class>

    <class name="org.flymine.model.genomic.ThreePrimeUTR"  extends="org.flymine.model.genomic.UTR" is-interface="true">
    </class>

    <class name="org.flymine.model.genomic.Region" is-interface="true"/>

    <class name="org.flymine.model.genomic.GenomeRegion" is-interface="true"
           extends="org.flymine.model.genomic.Region"/>

    <class name="org.flymine.model.genomic.Assembly" is-interface="true"
           extends="org.flymine.model.genomic.GenomeRegion">
    </class>

    <class name="org.flymine.model.genomic.Chromosome" is-interface="true"
           extends="org.flymine.model.genomic.GenomeRegion"/>
</classes>
