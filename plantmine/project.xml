<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="plantmine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>

  <sources>
    <source name="arabidopsis-tigrxml" type="intermine-items-xml-file" dump="true">
      <property name="tgt.xml.file" 
                location="/shared/data/arabidopsis/arabidopsis.org/items/tair_items.xml"/>
    </source>
    <source name="pombe-embl" type="biojava-flatfile" dump="true">
      <property name="src.data.dir" location="/shared/data/embl/pombe/"/>
      <property name="convertor.class"
                value="org.intermine.bio.dataconversion.BioJavaFlatFileConverter"/>
    </source>
    <source name="uniprot" type="uniprot">
      <property name="uniprot.organisms" value="3702 4896 284812"/>
      <property name="src.data.dir" location="/shared/data/uniprot/7.7/"/>
    </source>
    <source name="psi-intact" type="psi" dump="true">
      <property name="src.data.dir" location="/shared/data/psi/intact/2006-09-11"/>
      <property name="organisms" value="3702 4896 284812 "/>
    </source>
<!--
    <source name="go" type="go">
      <property name="src.data.file" 
                location="/shared/data/arabidopsis/gene_ontology/gene_ontology.obo"/>
    </source>
    <source name="go-annotation-gene-identifier" type="go-annotation" dump="true">
      <property name="ontologyfile" 
                location="/shared/data/arabidopsis/gene_ontology/gene_ontology.obo"/>
      <property name="geneattribute" value="identifier"/>
      <property name="src.data.dir" location="/shared/data/arabidopsis/go-annotation"/>
    </source>
-->
    <source name="inparanoid-orthologue" type="orthologue">
      <property name="src.data.dir" location="/shared/data/orthologues/localInparanoid/"/>
    </source>
    <source name="entrez-organism" type="entrez-organism" dump="true">
      <property name="tgt.xml.file" location="build/organisms.xml"/>
    </source>
  </sources>

  <post-processing>
    <post-process name="create-references"/>
    <post-process name="update-orthologues"/>
    <post-process name="make-spanning-locations"/>
    <post-process name="create-chromosome-locations-and-lengths"/>
    <post-process name="create-overlap-relations-flymine">
      <property name="classesToIgnore" value="Primer, Contig, CDS, Chromosome, ChromosomeBand, Enhancer, Exon, FivePrimeUTR, Intron, MRNA, PCRProduct, PolyASite, Pseudogene, RRNA, RepeatRegion, Supercontig, ThreePrimeUTR, TranscriptionStartSite, TransposableElementInsertionSite"/>
    </post-process>

    <post-process name="do-sources"/>
  </post-processing>

</project>

