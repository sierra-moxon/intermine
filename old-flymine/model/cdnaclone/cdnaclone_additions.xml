<?xml version="1.0"?>
<classes>
  <class name="org.flymine.model.genomic.CDNAClone" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
    <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="clones"/>
  </class>
  <class name="org.flymine.model.genomic.CompositeSequence" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="url" type="java.lang.String"/>
    <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="compositeSeqs"/>
  </class>
  <class name="org.flymine.model.genomic.Gene" is-interface="true">
    <collection name="clones" referenced-type="org.flymine.model.genomic.CDNAClone" reverse-reference="gene" ordered="true"/>
    <collection name="compositeSeqs" referenced-type="org.flymine.model.genomic.CompositeSequence" reverse-reference="genes" ordered="true"/>
  </class>
</classes>

